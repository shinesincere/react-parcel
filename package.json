{
  "homepage": "https://github.com/shinesincere/react-parcel",
  "name": "react-parcel",
  "version": "1.0.0",
  "author": "shine sincere",
  "license": "MIT",
  "description": "Setting up a React project with Parcel as a bundler",
  "keywords": [
    "react",
    "parcel"
  ],
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html -d build --public-url ./",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cssnano": "^4.1.10",
    "node-sass": "^4.11.0",
    "postcss-modules": "^1.4.1",
    "typography": "^0.16.19"
  }
}
